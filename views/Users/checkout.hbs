   <script>

      function selectcoupon(couponId){
         let coupon = document.getElementById('couponvalues'+couponId).value
         console.log(coupon)
         document.getElementById('couponInput').value = coupon
      }

      $('#applycoupon').submit((e)=>{
         console.log('hell')
         e.preventDefault()
         $.ajax({
            url:'/apply-coupon',
            type:'post',
            data:$('#applycoupon').serialize(),
            success:(response)=>{
               if(response.status){

               swal(response.message,"",'info')
               $('#discount').html(response.percentage)
               gt = $('#realTotalAmount').html()
               gt = gt-(500*(response.percentage/100))
               da = 500*(response.percentage/100)

               $('#deductedAmount').val(da)
               $('#gtotal').html(gt)
               $('#gtotalaftercoupon').val(gt)
               $('#usedcouponcode').val(response.coupon)
               document.getElementById('realTotal').style.display = 'block'
                document.getElementById('realTotalAmount').style.display = 'block'
               }else{
                  swal(response.message,"",'info')
               }

            },
			error:(response)=>{
				location.href = '/error'
			}
         })
      })

      $("#checkout-form").submit((e)=>{
         e.preventDefault()
         $.ajax({
            url:'/place-order',
            method:'post',
            data:$("#checkout-form").serialize(),
            success:(response)=>{
               if(response.walletStatus){
                  if(response.noWalletBalance){
                     swal("Sorry You don't have enough balance in your wallet to place this order", "", "info")
                  }else{
                     location.href = '/ordersummary/'+response.insertedId
                  }
               }else{
                  if(response.codStatus){
                  location.href = '/ordersummary/'+response.insertedId
                  }else if(response.paypalStatus){
                     paypalForwarding(response)
                  }else{
                     razorpayPayment(response)
                  }
               }
            },
			error:(response)=>{
				location.href = '/error'
			}

         })
      })

      function razorpayPayment(order){
   
         var options = {
            "key": "rzp_test_4WthUxlOcAGkLi", // Enter the Key ID generated from the Dashboard
            "amount": order.amount, // Amount is in currency subunits. Default currency is INR. Hence, 50000 refers to 50000 paise
            "currency": "INR",
            "name": "Shoppee",
            "description": "Test Transaction",
            "image": "https://example.com/your_logo",
            "order_id": order.id, //This is a sample Order ID. Pass the `id` obtained in the response of Step 1
            "callback_url": "https://eneqd3r9zrjok.x.pipedream.net/",
            "handler": function (response){
               verifyPayment(response,order)
            },
            "prefill": {
               "name": order.user.name,
               "email": order.user.email,
               "contact": order.user.mobile
            },
            "notes": {
               "address": "Razorpay Corporate Office"
            },
            "theme": {
               "color": "#3399cc"
            }
         };
         var rzp1 = new Razorpay(options);
         rzp1.open();
      }

      function verifyPayment(payment,order){
         console.log('verify payment')
         $.ajax({
            url:'/verify-payment',
            data:{
               payment,
               order
            },
            method:'post',
            success:(response)=>{
               if(response.status){
                  location.href = '/ordersummary/'+response.orderId
               }else{
                  alert('Payment is not succesful')
               }
            },
			error:(response)=>{
				location.href = '/error'
			}
         })
      }

      function paypalForwarding(response){
         
         $.ajax({
            url:'/paypal',
            data:{
               paypalStatus:response.paypalStatus,
               orderId:response.orderId,
               grandTotal:response.grandTotal
            },
            type:'post',
            success:(response)=>{
               location.href = response
            }
         })
      }

      $("#address-form").submit((e)=>{
         e.preventDefault()
         console.log('helloll')
